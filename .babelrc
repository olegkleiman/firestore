{
  "presets" : [
      "es2015", "react", "stage-0"
    ],
  "plugins": [
    [
      "react-intl", {
        "messagesDir": "./assets/messages/"
      }
    ]
  ],
  "env": {
    "server": {
        "plugins": [
          [
            "css-modules-transform", {
              "extensions": [".css", ".scss"],
              "devMode": true
            }
          ]
        ]
    }
  }

}
